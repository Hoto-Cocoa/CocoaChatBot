{
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "env": {
        "es6": true
    },
    "rules": {
      "comma-dangle": [2,"never"],
      "no-console": 1,
      "no-constant-condition": 1,
      "no-empty": 1,
      "no-extra-semi": 1,
      "eqeqeq": [2,"smart"],
      "no-eval": 2,
      "no-redeclare": [1,{"builtinGlobals":true}],
      "no-self-compare": 2,
      "array-bracket-spacing": [1,"always",{"singleValue":true}],
      "brace-style": [1,"1tbs",{"allowSingleLine":true}],
      "comma-spacing": [1,{"after":true}],
      "no-spaced-func": 1,
      "keyword-spacing": [1,{ "overrides": { "if": { "after": false }, "for": { "after": false }, "while": { "after": false }, "catch": { "after": false } } } ],
      "spaced-comment": [1,"always",{"exceptions":["-"," ="," *"," +"],"markers":["!"]}]
    }
  }